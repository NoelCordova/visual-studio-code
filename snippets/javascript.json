{
	"Arrow function": {
		"prefix": "func",
		"body": [
			"const $1 = ($2) => {",
			"  $3",
			"}",
			"",
			"$4"
		],
		"description": "Basic structure for an arrow function"
	},
	
	"Console log": {
		"prefix": "clg",
		"body": [
			"console.log($1);",
			"$2"
		],
		"description": "Log output to console"
	},

	"Comment": {
		"prefix": "com",
		"body": [
			"// $1",
			"$2"
		],
		"description": "Single comment"
	},

	"Comment info": {
		"prefix": "comi",
		"body": [
			"// -----------------------------",
			"// $1",
			"// -----------------------------",
			"$2"
		],
		"description": "Comment with a space for information"
	}
}